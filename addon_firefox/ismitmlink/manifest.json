   "version": "1.0.14.1",
   "homepage_url": "https://codeberg.org/crimeflare/cloudflare-tor",
   "author": "Maslin Bossé",
   "permissions": [
      "storage",
      "unlimitedStorage"
   ],
   "icons": {
      "32": "icons/32.png"
   },
   "background": {
      "scripts": [
         "bg.js"
      ]
   },
   "content_scripts": [
